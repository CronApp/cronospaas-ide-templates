{
   "name":"cordova-plugin-mobile-phonegap",
   "version":"1.0.0",
   "plugins":[
	  {
         "id"   : "$cordovaAdMob ",
         "name" : "Cordova AdMob Ads Plugin",
		 "cmd"  : "https://github.com/floatinghotpot/cordova-plugin-admob.git",
		 "lib"  : "cordova-plugin-admob"
      },
	  {
         "id"   : "$cordovaAppRate ",
